pipeline {
	    agent any
	    stages {
	        stage('git repo & clean') {
	            steps {
	               bat "rmdir  /s /q ASE_Junit"
	                bat "git clone https://github.com/bhargavs09/ASE_Junit.git"
	                bat "mvn clean -f ASE_Junit"
	            }
	        }
	        stage('install') {
	            steps {
	                bat "mvn install -f ASE_Junit"
	            }
	        }
	        stage('test') {
	            steps {
	                bat "mvn test -f ASE_Junit"
	            }
	        }
	        stage('package') {
	            steps {
	                bat "mvn package -f ASE_Junit"
	            }
	        }
	    }
}

